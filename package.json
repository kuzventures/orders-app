  "license": "MIT",
  "scripts": {
    "dev": "npm run build:api && npm run start:all",
    "start:all": "nx run-many --target=serve --projects=api,client --parallel",
    "start:client": "nx serve client",
    "start:api": "nx serve api",
    "build:all": "nx run-many --target=build --projects=types,api,client",
    "build:client": "nx build client",
    "build:api": "nx build api",
    "build:types": "nx build types",
    "lint:all": "nx run-many --target=lint --projects=api,client,types",
    "test:all": "nx run-many --target=test --projects=api,client",
    "seed": "nx run api:serve --args=\"--seed\"",
    "clean": "nx reset",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v"
  },
